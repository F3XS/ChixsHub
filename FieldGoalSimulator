               end
            end
            rconsoleclear('s')
    end)
    
    
    
    
    
    -- Main
    MainSection:NewButton("100% accuracy", "you just gonna have high accuracy ", function()
        local mt = getrawmetatable(game)
    local old = mt.__namecall
    setreadonly(mt, false)
    mt.__namecall =
       newcclosure(
       function(remote, ...)
           args = {...}
           method = tostring(getnamecallmethod())
           if method == "FireServer" and tostring(remote) == "ServerEvent" and args[1] == "Accuracy" then
               args[2] = 1
               return old(remote, unpack(args))
           end
           return old(remote, ...)
       end
    )
    setreadonly(mt, true)
    end)
    MainSection:NewToggle("AutoKick", "Just an custom AutoFarm Position ;)", function(v)
        getgenv().AutoKick = v
        while true do
            if not getgenv().AutoKick then return end
            local Players = game:GetService("Players");
            local Char = Players.LocalPlayer.Character;
    
            Char.Ball.ServerEvent:FireServer("Start")
            wait(0.1)
        end
    end)
    MainSection:NewToggle("AutoBuy", "AutoBuys items", function(v)
        getgenv().AutoBuy = v
    
        while wait do
            if not getgenv().AutoBuy then return end
            local ServerEvent = game:GetService("ReplicatedStorage").ServerEvent;
            local Types = {"White", "Grass", "Wood", "Bronze", "Triangles", "Allstar", "Moon", "Water", "Fire", "Emerald", "Amethyst", "Cloudy", "Money", "Pink Camo", "Gloomy", "Lightning", "Magma", "Galaxy", "Fire & Ice"}
    
            for i,v in ipairs(Types) do
                ServerEvent:FireServer("Buy", "Balls", v)
                ServerEvent:FireServer("Buy", "Helmets", v)
                ServerEvent:FireServer("Buy", "Jerseys", v)
                ServerEvent:FireServer("Buy", "Cleats", v)
                wait(0.1)
            end
        end
    end)
    MainSection:NewToggle("AutoRebirth", "It Does what it says it Does", function(state)
        if state then
            getgenv().AutoRebirth = true
    
            spawn(function()
                while wait() do
                    if getgenv().AutoRebirth == true then
                        local args = {[1] = "Rebirth"}game:GetService("ReplicatedStorage").ServerEvent:FireServer(unpack(args))
                    end
                end
            end)
        else
            getgenv().AutoRebirth = false
    
            spawn(function()
                while wait() do
                    if getgenv().AutoRebirth == true then
                        local args = {[1] = "Rebirth"}game:GetService("ReplicatedStorage").ServerEvent:FireServer(unpack(args))
                    end
                end
            end)
        end
    end)
    local MainSection = Main:NewSection("Others")
    MainSection:NewButton("Open Rebirth UI", "üíóChixsüíó", function()
        game.Players.LocalPlayer.PlayerGui.RebirthGui["Frame"].Visible = true
    end)
    MainSection:NewButton("Open BallStore UI", "üíóChixsüíó", function()
        game.Players.LocalPlayer.PlayerGui.BallStoreGui["Frame"].Visible = true
    end)
    MainSection:NewButton("Open ClothingStore UI", "üíóChixsüíó", function()
        game.Players.LocalPlayer.PlayerGui.ClothingStoreGui["Buttons"].Visible = true
    end)
    
    
    
    
    
    -- AutoFarm
    AutoFarmSection:NewToggle("5 Yards AutoFarm", "idk why i added this shit 5 yards", function(state)
        if state then
            getgenv().Settings = {
                Farm = true,
                Yard = "5",
                Notification = false
            }
            
            local Character = game:GetService("Players").LocalPlayer.Character
            local TotalKicks = 0
            local Yards = {
                ["5"] = -215,
                ["10"] = -200,
                ["15"] = -185,
                ["20"] = -170,
                ["25"] = -155,
                ["30"] = -140,
                ["35"] = -125,
                ["40"] = -110,
                ["45"] = -95,
                ["50"] = -80,
                ["55"] = -65,
                ["60"] = -50,
                ["65"] = -35,
                ["70"] = -20,
                ["75"] = -5,
                ["80"] = 10,
                ["85"] = 25,
                ["90"] = 40,
                ["95"] = 55,
                ["100"] = 70,
            }
            
            Character.HumanoidRootPart.CFrame = CFrame.new(Yards[getgenv().Settings.Yard], -6, -470)
            
            while wait(3) do
                if getgenv().Settings.Farm then
                    Character.Ball.ServerEvent:FireServer("Start")
                    Character.Ball.ServerEvent:FireServer("Accuracy", 0.9999999999999999999)
                    TotalKicks = TotalKicks + 1
                    if getgenv().Settings.Notification then
                        game:GetService("StarterGui"):SetCore("SendNotification", {
                            Title = "üèà Field Goal Simulator", Text = TotalKicks .. " kick/s made.", Duration = 2
                        })
                    end
                else break
                end
            end
        else
            getgenv().Settings = {
                Farm = false,
                Yard = "5",
                Notification = false
            }
            
            local Character = game:GetService("Players").LocalPlayer.Character
            local TotalKicks = 0
            local Yards = {
                ["5"] = -215,
                ["10"] = -200,
                ["15"] = -185,
                ["20"] = -170,
                ["25"] = -155,
                ["30"] = -140,
                ["35"] = -125,
                ["40"] = -110,
                ["45"] = -95,
                ["50"] = -80,
                ["55"] = -65,
                ["60"] = -50,
                ["65"] = -35,
                ["70"] = -20,
                ["75"] = -5,
                ["80"] = 10,
                ["85"] = 25,
                ["90"] = 40,
                ["95"] = 55,
                ["100"] = 70,
            }
            
            Character.HumanoidRootPart.CFrame = CFrame.new(Yards[getgenv().Settings.Yard], -6, -470)
            
            while wait(3) do
                if getgenv().Settings.Farm then
                    Character.Ball.ServerEvent:FireServer("Start")
                    Character.Ball.ServerEvent:FireServer("Accuracy", 0.9999999999999999999)
                    TotalKicks = TotalKicks + 1
                    if getgenv().Settings.Notification then
                        game:GetService("StarterGui"):SetCore("SendNotification", {
                            Title = "üèà Field Goal Simulator", Text = TotalKicks .. " kick/s made.", Duration = 2
                        })
                    end
